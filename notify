<script type="text/javascript">
	$('#prova-social').hide();
    var $doc = $('html, body');
    $('.deslisar').click(function() {
        $doc.animate({
            scrollTop: $( $.attr(this, 'href') ).offset().top
        }, 2000);
        return false;
    });

